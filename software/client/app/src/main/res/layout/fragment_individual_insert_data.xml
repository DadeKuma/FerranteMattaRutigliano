<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layout_individual_connect_external_device"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.individual.IndividualInsertDataFragment"
    tools:layout_editor_absoluteY="81dp">

    <TextView
        android:id="@+id/text_individual_connect_external_device"
        android:layout_width="169dp"
        android:layout_height="0dp"
        android:layout_marginTop="69dp"
        android:layout_marginBottom="128dp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/button_individual_connect_external_device"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_individual_connect_external_device"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="179dp"
        android:text="Reconnect"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_individual_connect_external_device" />
</android.support.constraint.ConstraintLayout>